<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fiveguys.com.wishcraftapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/wc_logo_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AliTest"></activity>
        <activity android:name=".MyProfile" />
        <activity android:name=".ViewFriendProfile" />
        <activity android:name=".Feed" />
        <activity android:name=".CreateProfile" />
        <activity android:name=".ProfileSearch" />
        <activity android:name=".FeedButtons" />
        <activity android:name=".Settings" />
        <activity android:name=".ActivityFeedTest" />
        <activity android:name=".ItemSearch" />
        <activity
            android:name=".Login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>